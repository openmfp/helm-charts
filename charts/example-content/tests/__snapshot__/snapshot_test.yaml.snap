              livenessProbe:
                failureThreshold: 1
                httpGet:
                  path: /healthz
                  port: 8090
                periodSeconds: 10
              name: RELEASE-NAME-example-content
              ports:
                - containerPort: 8080
                  name: http
                  protocol: TCP
                - containerPort: 9090
                  name: metrics
                  protocol: TCP
                - containerPort: 8090
                  name: health-port
                  protocol: TCP
              readinessProbe:
                httpGet:
                  path: /readyz
                  port: 8090
                initialDelaySeconds: 5
                periodSeconds: 10
              resources:
                limits:
                  memory: 512Mi
                requests:
                  cpu: 40m
                  memory: 50Mi
              securityContext:
                readOnlyRootFilesystem: true
                runAsNonRoot: true
                seccompProfile:
                  type: RuntimeDefault
              startupProbe:
                failureThreshold: 30
                httpGet:
                  path: /readyz
                  port: 8090
                periodSeconds: 10
              volumeMounts:
                - mountPath: /var/cache/nginx
                  name: cache-volume
                  readOnly: false
                - mountPath: /tmp
                  name: tmp-volume
                  readOnly: false
                - mountPath: /var/run
                  name: run-volume
                  readOnly: false
          securityContext:
            runAsNonRoot: true
            seccompProfile:
              type: RuntimeDefault
          serviceAccountName: RELEASE-NAME-example-content
          terminationGracePeriodSeconds: 10
          volumes:
            - emptyDir: {}
              name: tmp-volume
            - emptyDir: {}
              name: cache-volume
            - emptyDir: {}
              name: run-volume
