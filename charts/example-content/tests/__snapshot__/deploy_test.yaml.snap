matches the snapshot:
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      labels:
        app: RELEASE-NAME-example-content
      name: RELEASE-NAME-example-content
    spec:
      revisionHistoryLimit: 3
      selector:
        matchLabels:
          app: RELEASE-NAME-example-content
      strategy:
        rollingUpdate:
          maxSurge: 5
          maxUnavailable: 0
      template:
        metadata:
          labels:
            app: RELEASE-NAME-example-content
        spec:
          containers:
            - automountServiceAccountToken: true
              env:
                - name: PORT
                  value: "8080"
              image: ghcr.io/openmfp/example-content:1.0.0
              imagePullPolicy: Always
              name: RELEASE-NAME-example-content
              ports:
                - containerPort: 8080
                  name: http
                  protocol: TCP
                - containerPort: 8080
                  name: metrics
                  protocol: TCP
                - containerPort: 8081
                  name: health-port
                  protocol: TCP
              resources:
                limits:
                  cpu: 100m
                  memory: 512Mi
                requests:
                  cpu: 40m
                  memory: 50Mi
              securityContext:
                readOnlyRootFilesystem: true
                runAsNonRoot: true
                seccompProfile:
                  type: RuntimeDefault
              serviceAccountName: RELEASE-NAME
              volumeMounts:
                - mountPath: /var/cache/nginx
                  name: tmp-volume
                  readOnly: false
                - mountPath: /var/run
                  name: run-volume
                  readOnly: false
          securityContext:
            fsGroup: 2000
            runAsGroup: 3000
            runAsUser: 1000
          serviceAccountName: RELEASE-NAME-example-content
          volumes:
            - emptyDir: {}
              name: tmp-volume
            - emptyDir: {}
              name: run-volume
