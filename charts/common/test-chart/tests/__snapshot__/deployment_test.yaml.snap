test extras:
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      name: RELEASE-NAME-test-chart
    spec:
      revisionHistoryLimit: 3
      selector:
        matchLabels:
          app: RELEASE-NAME-test-chart
      template:
        spec:
          containers:
            - args:
                - --metrics-bind-address=8090
                - --health-probe-bind-address=8090
                - --log-level=warn
                - --region=local
                - --environment=local
                - --image-tag=0.1.0
                - --image-name="ghcr.io/your-org/your-image"
                - --arg1=value1
                - --arg2=value2
              image: ghcr.io/your-org/your-image:0.1.0
              name: RELEASE-NAME-test-chart
              ports:
                - containerPort: 8080
                  name: http
                  protocol: TCP
                - containerPort: 9090
                  name: metrics
                  protocol: TCP
                - containerPort: 8090
                  name: health-port
                  protocol: TCPIfNotPresent
              resources:
                limits:
                  cpu: 100m
                  memory: 512Mi
                requests:
                  cpu: 40m
                  memory: 50Mi
          env:
            - name: MY_ENV
              value: my-value
          securityContext:
            readOnlyRootFilesystem: true
            runAsNonRoot: true
            seccompProfile:
              type: RuntimeDefault
          terminationGracePeriodSeconds: 10
          volumeMounts:
            - mountPath: /mnt/my-volume
              name: my-volume
            - mountPath: /mnt/my-volume2
              name: my-volume2
          volumes:
            - emptyDir: {}
              name: my-volume
            - emptyDir: {}
              name: my-volume2
